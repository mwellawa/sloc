PROCEDURE "sloc.db.procedures::getPostcode" (

in	im_postcode nvarchar(4),
	im_state nvarchar(3),
out ex_postcode nvarchar(4)

)
   LANGUAGE SQLSCRIPT
   SQL SECURITY INVOKER AS
BEGIN

   /*************************************
       Write your procedure logic 
   *************************************/
   select "postcode" into ex_postcode from "sloc.db.data::model.Postcode"
   where "postcode" = im_postcode and
		 "state.state" = im_state;
   
END